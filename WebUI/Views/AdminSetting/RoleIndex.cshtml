
@using Inv.WebUI.Resources
@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<style>

    .fontitm4user {
        right: 6px;
        float: left;
        font-size: 23px;
        margin: 7px 0px 0px -16px;
        position: absolute;
        z-index: 99
    }

    .borderred {
        border: solid 1px #f3f3f3 !important;
        border-radius: 30px;
        padding: 12px 11px;
        width: 98% !important;
        margin: 0.5%;
        background-color: #e9d8c7 !important;
        box-shadow: 0px 4px 6px 0px #cabfbf;
    }

    .disabledDiv {
        pointer-events: none;
        opacity: 0.6;
    }
</style>
<!-- Filter Container -->
<div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 margingred  borderred">
    <div class="card bg-light-blue">
        <div class="card-body">
            <div class="row">
                <div class="col-12 col-md-4 display_none">

                    <label class="col-lg-2 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">@SystemResource.App_Company</label>
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xl-8">
                        <select id="txtComp" name="" class="form-control input-sm addable editable">
                            <option value="null"> @SystemResource.App_Company </option>
                        </select>
                    </div>

                </div>
                <div class="col-12 col-md-4 display_none">
                    <label class="col-lg-2 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">@SystemResource.Branch</label>
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xl-8">
                        <select id="txtBranch" name="" class="form-control input-sm addable editable">
                            <option value="null"> @SystemResource.Branch </option>
                        </select>
                    </div>
                </div>

                <div class=" col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12" id="GridDetails">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3">
                        <input id="searchbutmemreport" name="" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.Search">
                    </div>

                    <button id="btnAdd" type="button" class="btn btn-success   float_left_right ">
            <span class="glyphicon glyphicon-plus"></span> @SystemResource.App_Add
        </button>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                        <table id="divGridDetails"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="col-lg-12 margingred  borderred  display_none" id="data_lebel">
    <div class="col-lg-12 col-md-6 col-sm-6 col-xl-6 col-xs-6 ">

        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 float_left_right">
            <button id="btnSave" type="button" class="btn btn-success display_none  ">
                @SystemResource.App_save   <span class="glyphicon glyphicon-floppy-saved"></span>
            </button>
            <button id="btnBack" type="button" class="btn btn-warning  display_none ">
                <span class="glyphicon glyphicon-save"></span> @SystemResource.App_back
            </button>
            <button id="btnEdit" type="button" class="btn btn-primary    float_left_right">
                <span class="glyphicon glyphicon-save"></span> @SystemResource.App_Update
            </button>
            

        </div>
    </div>
    <div class="col-lg-12">
        <br />
    </div>




    <div class="col-lg-12">


    </div>


    <div id="div_BasicData" class="col-lg-12">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">الاسم بالعربي</label>
            <div class="col-lg-5 col-md-2 col-sm-2 col-xl-2 col-xs-2">
                <input type="hidden" class="form-control input-sm addable editable" name="RoleId" id="txtRoleId">
                <input type="text" class="form-control input-sm addable editable" name="DescA" id="txtDescA">
            </div>
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 "> الاسم E</label>
            <div class="col-lg-5 col-md-5 col-sm-5 col-xl-5 col-xs-5">
                <input type="text" class="form-control input-sm addable editable" name="DescE" id="txtDescE">
            </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 "> IsAvailable  </label>

            <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2">
                <input type="checkbox" class="checkbox" id="txtIsAvailable" disabled name="IsAvailable">
            </div>
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">IsShowable</label>
            <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2">
                <input type="checkbox" class="checkbox" id="txtIsShowable" disabled name="IsShowable">
            </div>
             
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">RoleType</label>
            <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2">

                <select class="form-control input-sm addable editable" name="RoleType" id="txtRoleType">
                    <option value="1">مش عارف</option>
                    <option value="2">صلاحيات مستخدم</option>
                    <option value="3">صلاحيات فرع</option>
                </select>
            </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 pading_2Processes2 ">  الملاحظات </label>
            <div class="col-lg-10 col-md-2 col-sm-2 col-xl-2 col-xs-2">
                <input type="text" class="form-control input-sm addable editable" name="Remarks" id="txtRemarks">
            </div>
        </div>
    </div>


</div>
<div class="col-lg-1"> </div>
<div id="DivMassage" style="background-color: #1d2920!important; margin: 1%;color:#ffffff; text-align: center; font-weight: bold;" class="col-lg-10 col-md-10 col-sm-10 col-xl-10 col-xs-10  margingred  borderred display_none">
    <h4 id="Text_Massage" style="">@SystemResource.App_Print</h4>
