@section Script{
    <script src="~/ClientApp/GeneralLedger/GLDefAccount.js"></script>
    <script>
        !function ($) {


            $(document).on("click", "#left ul.menu li.parent > a > span.sign", function () {

                //$(this).find('i:first').toggleClass("glyphicon glyphicon glyphicon-minus");
                $(this).find('i:first').toggleClass("fas fa-minus-circle");
            });

            // Open Le current menu

            //$("#left ul.menu li.parent.active > a > span.sign").find('i:first').addClass("glyphicon glyphicon glyphicon-minus");
            $("#left ul.menu li.parent.active > a > span.sign").find('i:first').addClass("fas fa-minus-circle");
            $("#left ul.menu li.current").parents('ul.children').addClass("in");

        }(window.jQuery);
    </script>
}
