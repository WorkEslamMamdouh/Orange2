                                    <label class="col-4 align-self-center p-md-0">@SystemResource.Detailed</label>
                                    <div class="col-8 ">
                                        <input type="checkbox" class="checkbox" id="chkeck_Detailed" disabled name="" />
                                    </div>
                                </div>
                            </div>
