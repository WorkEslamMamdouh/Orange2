@using Inv.WebUI.Resources
@{
    ViewBag.Title = @SystemResource.Men_AccDefCustomer;
    Layout = "~/Views/Shared/Layout.cshtml";
}
<!-- Side Icon Bar -->
<div class="main-iconbar" id="Div_control">
    <button class="icon-bar d-flex justify-content-between btnadd" disabled id="btnadd">
        <i class="fas fa-folder-plus fa-fw side-icon ms-1"></i>
        <span class="align-self-center mb-3">  @SystemResource.App_Add</span>
    </button>
    <button class="icon-bar d-flex justify-content-between btnDelete" disabled id="btnDelete">
        <i class="fas fa-trash-alt fa-fw side-icon ms-1"></i>
        <span class="align-self-center mb-3"> @SystemResource.App_Delete</span>
    </button>
    <button class="icon-bar d-flex justify-content-between btnedite" disabled id="btnedite">
        <i class="fas fa-edit side-icon ms-1"></i>
        <span class="align-self-center mb-3">   @SystemResource.App_Update </span>
    </button>
    <button class="icon-bar d-flex justify-content-between btnsave display_none" id="btnsave">
        <i class="fas fa-save fa-fw side-icon ms-1"></i>
        <span class="align-self-center mb-3">   @SystemResource.App_save </span>
    </button>
    <button class="icon-bar d-flex justify-content-between btnback display_none" id="btnback">
        <i class="fas fa-backspace fa-fw side-icon ms-1"></i>
        <span class="align-self-center mb-3">  @SystemResource.App_back</span>
    </button>
</div>
<div class="row  gy-3" id="content_report">
    <div class="col-12 col-md-4 ">
        <div class="card bg-light-blue h-100">
            <div class="card-body">
                <div class="row">
                    <span data-toggle="collapse" class="d-flex justify-content-end align-content-start">
                        <a id="Refrash" class="Refrash" href="#" title="Refrash">
                            <span data-toggle="collapse">
                                <i id="RE" class="fas fa-sync-alt fs-5 text-blue btn-refresh"></i>
                            </span>
                        </a>
                    </span>
                    <div id="left" class="col-12 account-menu">
                        <ul id="menu-group-1" class="main-menu"></ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-12 col-md-8 ">
        <div class="card bg-light-blue h-100">
            <div class="card-body">
                <div class="row">
                    <div class="col-12 col-md-8">
                        <div class="form-group row">
                            <label class="col-2 align-self-center p-md-0">@SystemResource.App_account_numbe</label>
                            <div class="col-10 ">
                                <input id="txt_ACC_CODE" disabled name="ACC_CODE" type="text" class="form-control" placeholder=" @SystemResource.App_account_numbe ">
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3 offset-md-1">
