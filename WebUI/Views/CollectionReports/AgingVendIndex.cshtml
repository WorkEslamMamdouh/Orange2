                                <input type="date" class="form-control" name="txtFromDate" id="txtFromDate">
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row">
                            <label class="col-4 align-self-center p-md-0">@SystemResource.App_Status</label>
                            <div class="col-8 ">
                                <select id="txt_ID_APP_Type" name="" class="form-control">
                                    <option value="3"> @SystemResource.App_All </option>
                                    <option value="1"> @SystemResource.prepared </option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row">
                            <label class="col-4 align-self-center p-md-0">@SystemResource.App_indebtedness</label>
                            <div class="col-8 ">
                                <select id="txt_indebtedness" name="" class="form-control input-sm  text_Display">
                                    <option value="0"> @SystemResource.App_All </option>
                                    <option value="1"> @SystemResource.App_He_Owes_Him </option>
                                    <option value="2"> @SystemResource.App_an_indebtedness </option>
                                    <option value="3"> @SystemResource.App_Zero </option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        @* Empty for Responsive *@
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row">
                            <label class="col-4 align-self-center p-md-0">@SystemResource.APP_Category</label>
                            <div class="col-8 ">
                                <select id="txt_ID_APP_Category" name="" class="form-control">
                                    <option value="Null"> @SystemResource.choose_Cat </option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row">
                            <label class="col-4 align-self-center p-md-0">@SystemResource.App_Group</label>
                            <div class="col-8 ">
                                <select id="txt_ID_APP_Group" name="" class="form-control">
                                    <option value="Null">@SystemResource.Choose_group</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row g-0 d-flex justify-content-center align-items-center">
                            <label class="col-3 col-md-4 align-self-center p-md-0">@SystemResource.I_Vendor </label>
                            <div class="col-9 col-md-8">

                                <div class="search-content">
                                    <input type="text" class="form-control search-control" id="txt_VndCode">
                                    <button type="button" id="btnVnd" name="ColSearch" class="btn btn-main btn-search">
                                        <i class="fas fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="form-group row g-0">
                            <label class="align-self-center p-md-0"></label>
                            <div class="col-8">
                                <input type="text" class="form-control" disabled name="" id="txt_VndName">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

